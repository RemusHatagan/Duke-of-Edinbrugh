  turtle.right(45)
turtle.right(30)
for i in range(8):
  turtle.forward(100)
  turtle.right(45)
turtle.right(30)
for i in range(8):
  turtle.forward(100)
  turtle.right(45)
turtle.right(30)
for i in range(8):
  turtle.forward(100)
  turtle.right(45)
turtle.right(30)
for i in range(8):
  turtle.forward(100)
  turtle.right(45)
turtle.right(30)
for i in range(8):
  turtle.forward(100)
  turtle.right(45)
turtle.right(30)
for i in range(8):
  turtle.forward(100)
  turtle.right(45)
 
turtle.exitonclick()

Challenge 068

import turtle, random
 
number=50
 
for x in range(number):
  turtle.forward(random.randint(0,100))
  turtle.right(random.randint(0,180))
 
 
 
turtle.exitonclick()
